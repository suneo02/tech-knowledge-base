{
  "name": "report-util",
  "version": "1.0.0",
  "description": "A simple project for wkhtmltopdf export with ES6 syntax",
  "type": "module",
  "exports": {
    "./constants": {
      "types": "./dist/types/constants/index.d.ts",
      "import": "./dist/constants/index.mjs"
    },
    "./corpConfigJson": {
      "types": "./dist/types/corpConfigJson/index.d.ts",
      "import": "./dist/corpConfigJson/index.mjs"
    },
    "./format": {
      "types": "./dist/types/format/index.d.ts",
      "import": "./dist/format/index.mjs"
    },
    "./misc": {
      "types": "./dist/types/misc/index.d.ts",
      "import": "./dist/misc/index.mjs"
    },
    "./table": {
      "types": "./dist/types/table/index.d.ts",
      "import": "./dist/table/index.mjs"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "import": "./dist/types/index.mjs"
    },
    "./url": {
      "types": "./dist/types/url/index.d.ts",
      "import": "./dist/url/index.mjs"
    },
    "./tree": {
      "types": "./dist/types/tree/index.d.ts",
      "import": "./dist/tree/index.mjs"
    },
    "./style": {
      "default": "./src/style/index.less"
    }
  },
  "files": [
    "dist",
    "README.md",
    "src/style"
  ],
  "scripts": {
    "clean": "node ../../scripts/clean.cjs dist",
    "build": "npm run clean && vite build",
    "build:staging": "npm run clean && vite build --mode staging",
    "dev": "vite build --mode development --watch",
    "test": "vitest",
    "tsc": "tsc --noEmit",
    "lint": "pnpm -s eslint --ext .js src/",
    "lint:fix": "pnpm -s eslint --ext .js src/ --fix",
    "check:circular": "madge --circular --extensions ts,tsx src/",
    "format": "prettier --write \"src/**/*.js\"",
    "format:file": "prettier --write",
    "check-format": "prettier --check \"src/**/*.js\"",
    "validate": "pnpm run lint && pnpm run check-format"
  },
  "devDependencies": {
    "@types/jquery": "^1.8",
    "@types/node": "^22.14.1",
    "gel-types": "workspace:*",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "lz-string": "^1.5.0"
  },
  "peerDependencies": {
    "gel-types": "workspace:*"
  },
  "sideEffects": false
}
