{
  "name": "gel-api",
  "version": "1.0.0",
  "description": "Gel Web Utilities - Environment and Client Functions",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development --minify false --sourcemap",
    "clean": "node ../../scripts/clean.cjs dist",
    "build": "npm run clean && vite build",
    "build:staging": "npm run clean && vite build --mode staging",
    "typecheck": "tsc --noEmit",
    "tsc": "npm run typecheck",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build",
    "check:circular": "madge --circular --extensions ts,tsx src/"
  },
  "peerDependencies": {
    "antd": "^5.24.0",
    "axios": "^1.7.9",
    "gel-types": "workspace:*",
    "path-browserify": "^1.0.1",
    "qs": "^6.14.0",
    "react": ">=16.8.0",
    "typescript": ">=4.5.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.2.48",
    "gel-types": "workspace:*",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "gel",
    "web",
    "utilities",
    "environment"
  ],
  "author": "",
  "license": "UNLICENSED",
  "sideEffects": false,
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
