{
  "name": "gel-api",
  "version": "1.0.0",
  "description": "Gel Web Utilities - Environment and Client Functions",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development --minify false --sourcemap",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "tsc": "npm run typecheck",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "typescript": ">=4.5.0",
    "axios": "^1.7.9",
    "antd": "^5.24.0",
    "gel-types": "workspace:*",
    "gel-util": "workspace:*",
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^18.2.48",
    "@types/qs": "^6.9.18",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "gel",
    "web",
    "utilities",
    "environment"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {}
}
