{
  "name": "gel-workspace",
  "private": true,
  "version": "1.0.0",
  "description": "Gel Workspace",
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev": "turbo dev",
    "dev:ai-chat": "turbo dev --filter=ai-chat...",
    "dev:report-preview": "turbo dev --filter=report-preview...",
    "dev:report-print": "turbo dev --filter=report-print...",
    "build": "turbo build",
    "build:report-preview": "turbo build --filter=report-preview...",
    "build:report-print": "turbo build --filter=report-print...",
    "build:ai-chat": "turbo build --filter=ai-chat...",
    "build:company": "pnpm i && turbo build --filter=company...",
    "build:packages": "turbo build --filter='./packages/*'",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "lint": "turbo lint",
    "serve:ai-chat": "npx serve -s apps/ai-chat/dist",
    "serve:company": "npx serve -s apps/company/build",
    "tsc": "turbo tsc",
    "tsc:ai-chat": "turbo tsc --filter=ai-chat...",
    "tsc:report-preview": "turbo tsc --filter=report-preview...",
    "tsc:report-print": "turbo tsc --filter=report-print...",
    "tsc:company": "turbo tsc --filter=company...",
    "compress": "zip -r project_without_node_modules_$(date +%Y%m%d).zip . -x \"*node_modules*\" \"*.git*\" \"*.turbo*\"",
    "proxy": "node api-proxy-server.js",
    "deploy:all": "node scripts/build-and-deploy.js",
    "deploy:company": "node scripts/build-and-deploy.js company",
    "json:intl": "node scripts/jsonLikeFileIntl.cjs",
    "json:sort": "node scripts/jsonSort.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.9",
    "@storybook/html": "^8.6.12",
    "@storybook/html-webpack5": "^8.6.12",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.9",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.13",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "3.2.4",
    "@vitest/ui": "3.2.4",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint-plugin-storybook": "^0.12.0",
    "express": "^4.18.3",
    "http-proxy-middleware": "^2.0.6",
    "storybook": "^8.6.14",
    "turbo": "^2.5.3",
    "vitest": "3.2.4",
    "wait-on": "^8.0.2"
  },
  "dependencies": {
    "axios": "1.9.0"
  }
}
