{
  "name": "report-preview-ui",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/style.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite build --watch --mode development",
    "clean": "node ../../scripts/clean.cjs dist",
    "build": "npm run clean && vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "npm run clean && vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "story": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check:circular": "madge --circular --extensions ts,tsx src/"
  },
  "peerDependencies": {
    "axios": "^1.7.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@wind/icons": "^1.1.83",
    "@wind/wind-ui": "4.5.9",
    "ahooks": "^3.8.4",
    "classnames": "^2.5.1",
    "path-browserify": "^1.0.1",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "detail-page-config": "workspace:*",
    "gel-types": "workspace:*",
    "gel-api": "workspace:*",
    "gel-util": "workspace:*",
    "report-util": "workspace:*",
    "gel-ui": "workspace:*"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/classnames": "^2.3.4",
    "@types/markdown-it": "^14.1.2",
    "@types/lodash": "^4.17.15",
    "detail-page-config": "workspace:*",
    "gel-types": "workspace:*",
    "gel-api": "workspace:*",
    "gel-util": "workspace:*",
    "report-util": "workspace:*",
    "gel-ui": "workspace:*",
    "less": "^4.2.2",
    "typescript": "^5.2.2"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ]
