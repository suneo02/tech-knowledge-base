{
  "name": "gel-util",
  "version": "1.0.0",
  "description": "Gel Web Utilities - Environment and Client Functions",
  "main": "./dist/env.mjs",
  "module": "./dist/env.mjs",
  "types": "./dist/types/env/index.d.ts",
  "typesVersions": {
    "*": {
      "biz": [
        "./dist/types/biz/index.d.ts"
      ],
      "common": [
        "./dist/types/common/index.d.ts"
      ],
      "config": [
        "./dist/types/config/index.d.ts"
      ],
      "corp": [
        "./dist/types/corp/index.d.ts"
      ],
      "corpConfig": [
        "./dist/types/corpConfig/index.d.ts"
      ],
      "env": [
        "./dist/types/env/index.d.ts"
      ],
      "errorLogger": [
        "./dist/types/errorLogger/index.d.ts"
      ],
      "format": [
        "./dist/types/format/index.d.ts"
      ],
      "hooks": [
        "./dist/types/hooks/index.d.ts"
      ],
      "intl": [
        "./dist/types/intl/index.d.ts"
      ],
      "link": [
        "./dist/types/link/index.d.ts"
      ],
      "typeUtil": [
        "./dist/types/typeUtil/index.d.ts"
      ],
      "misc": [
        "./dist/types/misc/index.d.ts"
      ],
      "storage": [
        "./dist/types/storage/index.d.ts"
      ],
      "types": [
        "./dist/types/types.d.ts"
      ],
      "locales": [
        "./dist/types/locales/index.d.ts"
      ]
    }
  },
  "exports": {
    "./biz": {
      "types": "./dist/types/biz/index.d.ts",
      "import": "./dist/biz.mjs"
    },
    "./common": {
      "types": "./dist/types/common/index.d.ts",
      "import": "./dist/common.mjs"
    },
    "./config": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/config.mjs"
    },
    "./corp": {
      "types": "./dist/types/corp/index.d.ts",
      "import": "./dist/corp.mjs"
    },
    "./corpConfig": {
      "types": "./dist/types/corpConfig/index.d.ts",
      "import": "./dist/corpConfig.mjs"
    },
    "./env": {
      "types": "./dist/types/env/index.d.ts",
      "import": "./dist/env.mjs"
    },
    "./errorLogger": {
      "types": "./dist/types/errorLogger/index.d.ts",
      "import": "./dist/errorLogger.mjs"
    },
    "./format": {
      "types": "./dist/types/format/index.d.ts",
      "import": "./dist/format.mjs"
    },
    "./hooks": {
      "types": "./dist/types/hooks/index.d.ts",
      "import": "./dist/hooks.mjs"
    },
    "./intl": {
      "types": "./dist/types/intl/index.d.ts",
      "import": "./dist/intl.mjs"
    },
    "./link": {
      "types": "./dist/types/link/index.d.ts",
      "import": "./dist/link.mjs"
    },
    "./typeUtil": {
      "types": "./dist/types/typeUtil/index.d.ts",
      "import": "./dist/typeUtil.mjs"
    },
    "./misc": {
      "types": "./dist/types/misc/index.d.ts",
      "import": "./dist/misc.mjs"
    },
    "./storage": {
      "types": "./dist/types/storage/index.d.ts",
      "import": "./dist/storage.mjs"
    },
    "./types": {
      "types": "./dist/types/types.d.ts",
      "import": "./dist/types.mjs"
    },
    "./user": {
      "types": "./dist/types/user/index.d.ts",
      "import": "./dist/user.mjs"
    },
    "./locales": {
      "types": "./dist/types/locales/index.d.ts",
      "import": "./dist/locales.mjs"
    },
    "./locales/*": "./dist/locales/*",
    "./intl/createNamespaceI18n": {
      "types": "./dist/types/intl/createNamespaceI18n.d.ts",
      "import": "./dist/intl/createNamespaceI18n.mjs"
    },
    "./intl/dynamicI18n": {
      "types": "./dist/types/intl/dynamicI18n.d.ts",
      "import": "./dist/intl/dynamicI18n.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "node ../../scripts/clean.cjs dist",
    "dev": "vite build --watch --mode development",
    "build": "npm run clean && vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "npm run clean && vite build --mode staging",
    "typecheck": "tsc --noEmit",
    "tsc": "npm run typecheck",
    "lint": "pnpm -s eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run clean && npm run build",
    "intl:entry2json": "node scripts/intlTools/entry2Json.js",
    "intl:migrate-locale": "node scripts/migrate-locale-to-json.js src/intl/locales/iml_en.js src/intl/locales/iml_cn.js",
    "intl:split": "node scripts/jsonTools/split-locale-json.js",
    "intl:build": "node scripts/intl-build.js",
    "rime:prune2-to-en": "node scripts/jsonTools/rime-prune2-to-en.js",
    "check:circular": "madge --circular --extensions ts,tsx src/"
  },
  "peerDependencies": {
    "axios": "^1.7.9",
    "gel-api": "workspace:*",
    "gel-types": "workspace:*",
    "i18next": "^24.2.2",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "path-browserify": "^1.0.1",
    "qs": "^6.14.0",
    "react": ">=16.8.0",
    "typescript": ">=4.5.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.19.2",
    "@types/path-browserify": "^1.0.3",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.2.48",
    "gel-api": "workspace:*",
    "gel-types": "workspace:*",
    "jsdom": "^26.1.0",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "gel",
    "web",
    "utilities",
    "environment"
  ],
  "author": "",
  "license": "UNLICENSED",
  "sideEffects": false,
  "dependencies": {
    "i18next-browser-languagedetector": "^8.2.0",
    "react-i18next": "^11.18.6"
  }
}
