    },
    "ReportDetailCustomNodeJson": {
      "description": "自定义节点\n\n实在无法用 json 配置的节点\n\n根据对应的 key 来做渲染",
      "properties": {
        "api": {
          "type": "string"
        },
        "apiOptions": {
          "additionalProperties": false,
          "properties": {
            "method": {
              "enum": ["GET", "POST"],
              "type": "string"
            }
          },
          "type": "object"
        },
        "apiType": {
          "const": "operation",
          "description": "接口类型，默认是 gel/detail/company operation 即为 operation/get/...",
          "type": "string"
        },
        "extraParams": {
          "additionalProperties": {
            "type": "string"
          },
          "properties": {
            "companyCode": {
              "type": "string"
            }
          },
          "required": ["companyCode"],
          "type": "object"
        },
        "extraPayload": {
          "$ref": "#/definitions/Record%3Cstring%2Cstring%3E"
        },
        "hiddenTitle": {
          "type": "boolean"
        },
        "key": {
          "$ref": "#/definitions/TCorpDetailNodeKey"
        },
        "title": {
          "type": "string"
        },
        "titleIntl": {
          "type": ["string", "number"]
        },
        "type": {
          "const": "custom",
          "type": "string"
        }
      },
      "required": ["key", "title", "type"],
      "type": "object"
    },
    "TCorpDetailNodeKey": {
      "anyOf": [
        {
          "$ref": "#/definitions/corpDetailBaseInfoKey"
        },
        {
          "$ref": "#/definitions/corpDetailBusinessDataKey"
        },
        {
          "$ref": "#/definitions/corpDetailFinanceKey"
