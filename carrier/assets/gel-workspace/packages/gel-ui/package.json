{
  "name": "gel-ui",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./variables.less": "./dist/styles/shared/variables.less",
    "./dist/index.css": "./dist/index.css",
    "./mixin/index.less": "./dist/styles/mixin/index.less",
    "./mixin/gradient-bottom.less": "./dist/styles/mixin/gradient-bottom.less"
  },
  "scripts": {
    "dev": "vite build --watch --mode development --minify false --sourcemap",
    "clean": "node ../../scripts/clean.cjs dist",
    "build": "npm run clean && vite build",
    "build:staging": "npm run clean && vite build --mode staging",
    "preview": "vite preview",
    "lint": "pnpm -s eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "story": "storybook dev -p 6006",
    "build-story": "storybook build",
    "check:circular": "madge --circular --extensions ts,tsx src/"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/x": "^1.0.5",
    "@wind/icons": "^1.1.83",
    "@wind/rime-icons": "^1.0.0",
    "@wind/wind-ui": "4.5.9",
    "@wind/wind-ui-form": "^4.0.10",
    "@wind/wind-ui-table": "^2.2.44",
    "@wind/wui-alice-logo": "^1.0.3",
    "ahooks": "^3.8.4",
    "antd": "^5.24.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "gel-api": "workspace:*",
    "gel-types": "workspace:*",
    "gel-util": "workspace:*",
    "path-browserify": "^1.0.1",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6"
  },
  "dependencies": {
    "@wind/alice-bitmap-animation": "^1.0.6",
    "dompurify": "^3.2.6",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.30",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "gel-api": "workspace:*",
    "gel-types": "workspace:*",
    "gel-util": "workspace:*",
    "less": "^4.2.2",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.2.2"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ]
}
