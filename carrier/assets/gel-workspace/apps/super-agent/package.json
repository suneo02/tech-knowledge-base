    "ahooks": "^3.8.5",
    "antd": "^5.25.4",
    "axios": "^1.12.2",
    "babel-plugin-import": "^1.13.8",
    "classnames": "^2.5.1",
    "gel-api": "workspace:*",
    "gel-types": "workspace:*",
    "gel-ui": "workspace:*",
    "gel-util": "workspace:*",
    "markdown-it": "^14.1.0",
    "react": "^18",
    "react-countup": "^6.5.3",
    "react-dom": "^18",
    "react-redux": "^9.2.0",
