{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["ie >= 8", "safari >= 5.1"],
          "esmodules": false
        },
        "useBuiltIns": false,
        "modules": "auto"
      }
    ]
  ],
  "plugins": [["@babel/plugin-transform-template-literals", { "loose": true }]]
}
