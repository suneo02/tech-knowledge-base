{
  "name": "report-preview",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "node ../../scripts/clean.cjs dist",
    "dev": "vite --host",
    "dev:debug": "vite --host",
    "build": "npm run clean && tsc && vite build",
    "build:staging": "npm run clean && tsc && vite build --mode staging",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "preview": "vite preview",
    "lint": "pnpm -s eslint src --ext ts,tsx",
    "lint:fix": "pnpm -s eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "export:pdf": "node --loader ts-node/esm ./script/export.ts 'http://localhost:3020/index.html?companyCode=1173319566#/creditrpprint' output.pdf",
    "check:circular": "madge --circular --extensions ts,tsx src/"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "@wind/icons": "^1.1.83",
    "@wind/wind-ui": "4.5.9",
    "ahooks": "^3.8.4",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "detail-page-config": "workspace:*",
    "gel-api": "workspace:*",
    "gel-types": "workspace:*",
    "gel-ui": "workspace:*",
    "gel-util": "workspace:*",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "path-browserify": "^1.0.1",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^11.18.6",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.2.0",
    "report-util": "workspace:*",
    "report-preview-ui": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "cross-env": "^7.0.3",
    "less": "^4.2.0",
    "prettier": "^3.2.0",
    "puppeteer-core": "^22.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
