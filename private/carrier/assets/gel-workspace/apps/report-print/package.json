{
  "name": "report-print",
  "version": "1.0.0",
  "description": "A simple project for wkhtmltopdf export with ES6 syntax",
  "main": "dist/bundle.js",
  "type": "module",
  "scripts": {
    "clean": "node ../../scripts/clean.cjs dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "build:staging": "npm run clean && cross-env NODE_ENV=staging webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev": "concurrently \"pnpm run watch\" \"node server.js\"",
    "dev:debug": "concurrently \"pnpm run watch\" \"node server.js\"",
    "test": "vitest",
    "tsc": "tsc --noEmit",
    "check:circular": "madge --circular --extensions js,jsx src/",
    "lint": "pnpm -s eslint --ext .js src/",
    "lint:fix": "pnpm -s eslint --ext .js src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:file": "prettier --write",
    "check-format": "prettier --check \"src/**/*.js\"",
    "validate": "pnpm run lint && pnpm run check-format",
    "export:credit:test:cn": "node scripts/export.cjs --env test --language cn",
    "export:credit:main:cn": "node scripts/export.cjs --env main --language cn",
    "export:credit:batch": "node scripts/export.cjs --batch true",
    "export:creditevaluation:batch:local": "node scripts/export.cjs --report creditevaluation --batch true --env local --pattern NoIgwg9gtgDghgOwJ4EkEDMIgDQgEICuAzkWpjvgKYA2ECA5mgCbEAuATkhQMoAWc7SrwjUmldj36Dho8QCVKMCO1aSBQkWPZlocVgEs6FSOxhh%2BDSjooBRBK3Ex2%2BopQAKBAEbV9AY32sXLh86jJaAIIITGBwMAFw1JD2zp4EBka4NgCOBAFI5oj0VhjKUHqGCBQAKnAAHm5wSOIgALpAA%3D",
    "export:credit:batch:local": "node scripts/export.cjs --batch true --env local --pattern NoIgwg9gtgDghgOwJ4EkEDMIgDQgEICuAzkWpjvgKYA2ECA5mgCbEAuATkhQMoAWc7SrwjUmldj36Dho8QCVKMCO1aSBQkWPZlocVgEs6FSOxhh%2BDSjooBRBK3Ex2%2BopQAKBAEbV9AY32sXLh86jJaAIIITGBwMAFw1JD2zp4EBka4NgCOBAFI5oj0VhjKUHqGCBQAKnAAHm5wSOIgALpAA%3D",
    "export:creditevaluation:batch:test": "node scripts/export.cjs --report creditevaluation --batch true --env test --pattern NoIgwg9gtgDghgOwJ4EkEDMIgDQgEICuAzkWpjvgKYA2ECA5mgCbEAuATkhQMoAWc7SrwjUmldj36Dho8QCVKMCO1aSBQkWPZlocVgEs6FSOxhh%2BDSjooBRBK3Ex2%2BopQAKBAEbV9AY32sXLh86jJaAIIITGBwMAFw1JD2zp4EBka4NgCOBAFI5oj0VhjKUHqGCBQAKnAAHm5wSOIgALpAA%3D",
    "export:credit:batch:test": "node scripts/export.cjs --batch true --env test --pattern NoIgwg9gtgDghgOwJ4EkEDMIgDQgEICuAzkWpjvgKYA2ECA5mgCbEAuATkhQMoAWc7SrwjUmldj36Dho8QCVKMCO1aSBQkWPZlocVgEs6FSOxhh%2BDSjooBRBK3Ex2%2BopQAKBAEbV9AY32sXLh86jJaAIIITGBwMAFw1JD2zp4EBka4NgCOBAFI5oj0VhjKUHqGCBQAKnAAHm5wSOIgALpAA%3D",
    "export:credit:batch:main": "node scripts/export.cjs --batch true --env main  --pattern NoIgwg9gtgDghgOwJ4EkEDMIgDQgEICuAzkWpjvgKYA2ECA5mgCbEAuATkhQMoAWc7SrwjUmldj36Dho8QCVKMCO1aSBQkWPZlocVgEs6FSOxhh%2BDSjooBRBK3Ex2%2BopQAKBAEbV9AY32sXLh86jJaAIIITGBwMAFw1JD2zp4EBka4NgCOBAFI5oj0VhjKUHqGCBQAKnAAHm5wSOIgALpAA%3D",
    "storybook": "storybook dev -p 6007 --no-open",
    "build-storybook": "storybook build"
  },
