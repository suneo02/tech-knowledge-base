{
  "name": "ai-ui",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite build --watch --mode development",
    "clean": "node ../../scripts/clean.cjs dist",
    "build": "npm run clean && vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "npm run clean && vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tsc": "tsc --noEmit",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "check:circular": "madge --circular --extensions ts,tsx src/"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@ant-design/x": "^1.0.5",
    "@wind/chart-builder": "^1.1.6",
    "@wind/icons": "^1.1.83",
    "@wind/wind-ui": "4.5.9",
    "@wind/wind-ui-form": "^4.0.10",
    "@wind/wind-ui-table": "^2.2.44",
    "@wind/wui-alice-logo": "^1.0.3",
    "ahooks": "^3.8.4",
    "antd": "^5.24.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "gel-api": "workspace:*",
    "gel-ui": "workspace:*",
    "gel-util": "workspace:*",
    "markdown-it": "^14.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.9",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "less": "^4.2.2",
    "typescript": "^5.2.2",
    "gel-api": "workspace:*",
    "gel-ui": "workspace:*",
    "gel-util": "workspace:*",
    "@types/lodash-es": "^4.17.12"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
